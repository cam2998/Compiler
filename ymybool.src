decl main() : int;
decl x,y,z : int;

impl main(){
  x = 1;
  y = 2;
  z = 3;
  if (x < y && y < z && x + y == z) { put("first true\n"); };
  if (x > y || (y < z && x + y == z)) { put("second true\n"); };
  if (x > y || y > z) { put("third true\n"); };
  if ( !(x > y || y > z) ) { put("forth true\n"); };
};
